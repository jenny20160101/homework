<section class="row">
<h2>Survey</h2>
</section>
<section class="row">
<%= if @demographic do %>
    <h3>Demographic</h3>
    <ul>
    <li>Year of birth: <%= @demographic.year_of_birth %></li>
    <li>Gender: <%= @demographic.gender %>
    </ul>
    <% else %>
    <%= live_component @socket,
 PentoWeb.DemographicLive.FormComponent,
user: @current_user,
id: "demographic-form-#{@current_user.id}"
%>
<% end %>
</section>
